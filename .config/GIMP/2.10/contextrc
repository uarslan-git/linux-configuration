(background (color-rgb 0 0 0))
(brush "2. Hardness 025")
(dynamics "Dynamics Off")
(mybrush "100% Opaque")
(pattern "Pine")
(gradient "Abstract 1")
(palette "Standard")
(font "")
(tool-preset "Core Pointer")

